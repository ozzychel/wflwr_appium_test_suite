{
    "name": "wflwr_appium_test_suite",
    "version": "1.0.0",
    "description": "wflwr appium based test suite for android / ios",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ozzychel/wflwr_appium_test_suite.git"
    },
    "author": "Andrey Petunin",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ozzychel/wflwr_appium_test_suite/issues"
    },
    "homepage": "https://github.com/ozzychel/wflwr_appium_test_suite#readme",
    "eslintIgnore": ["/config/*.ts", "wdio.conf.ts"],
    "scripts": {
        "ios.app": "wdio config/wdio.ios.app.conf.ts --suite consent",
        "android.app": "wdio config/wdio.android.app.conf.ts --suite consent",
        "install": "wdio run wdio.conf.ts --suite install",
        "demo": "wdio run wdio.conf.ts --suite consent --suite signup --suite nav",
        "dev": "wdio run wdio.conf.ts --suite dev",
        "nav": "wdio run wdio.conf.ts --suite nav",
        "registration": "wdio run wdio.conf.ts --suite registration",
        "signup": "wdio run wdio.conf.ts --suite signup",
        "consent": "wdio run wdio.conf.ts --suite consent",
        "permissions": "wdio run wdio.conf.ts --suite permissions",
        "lint": "eslint appTest",
        "lint-fix": "eslint appTest --fix"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "@wdio/appium-service": "^7.26.0",
        "@wdio/cli": "^7.26.0",
        "@wdio/jasmine-framework": "^7.26.0",
        "@wdio/local-runner": "^7.26.0",
        "@wdio/spec-reporter": "7.24.0",
        "dotenv": "^16.3.1",
        "eslint": "^8.51.0",
        "eslint-plugin-wdio": "^8.20.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    }
}
